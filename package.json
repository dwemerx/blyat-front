{
  "name": "front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn build:css && next build && next export -o build",
    "build:css": "tailwindcss build src/styles/tailwind.css -o index.css",
    "dev": "concurrently yarn:dev:*",
    "dev:next": "next dev",
    "start": "next start",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll",
    "coverage": "react-scripts test --coverage --watchAll",
    "lint": "concurrently yarn:lint:*",
    "lint:pretty-quick": "pretty-quick --staged",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "fix": "concurrently yarn:fix:*",
    "fix:eslint": "yarn lint:eslint --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "next": "latest",
    "next-images": "^1.6.2",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-scripts": "^4.0.0",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.5"
  },
  "license": "MIT",
  "author": "Va",
  "description": "Next App",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx|md)": [
      "yarn lint:pretty-quick",
      "yarn lint:eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen --silent --forceExit --detectOpenHandles",
      "pre-commit": "lint-staged"
    }
  }
}
